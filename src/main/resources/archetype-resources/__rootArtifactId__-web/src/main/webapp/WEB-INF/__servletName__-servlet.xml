            <list>
                <ref bean="viewResolver" />
#if($framework.contains('freemarker'))
                <ref bean="freeMarkerViewResolver" />
#end
#if($framework.contains('thymeleaf'))
                <ref bean="thymeleafViewResolver" />
#end
            </list>
        </property>
    </bean>
